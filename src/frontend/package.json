{
    "name": "hippobox-frontend",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "predev": "npm run generate:api:optional",
        "dev": "vite build --watch",
        "dev:vite": "vite",
        "prebuild": "npm run generate:api:optional",
        "build": "tsc -b && vite build",
        "preview": "vite preview",
        "generate:api": "openapi-typescript http://localhost:8000/openapi.json -o src/api/openapi.ts",
        "generate:api:optional": "node scripts/generate-openapi.mjs",
        "format": "prettier . --write --loglevel warn",
        "format:check": "prettier . --check"
    },
    "dependencies": {
        "@tanstack/react-query": "^5.59.0",
        "dompurify": "^3.0.8",
        "highlight.js": "^11.9.0",
        "i18next": "^23.15.1",
        "lucide-react": "^0.562.0",
        "marked": "^9.1.6",
        "marked-highlight": "^2.1.2",
        "marked-katex-extension": "^3.0.0",
        "openapi-fetch": "^0.10.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-i18next": "^14.1.0",
        "react-router-dom": "^7.1.0"
    },
    "devDependencies": {
        "@types/node": "^22.19.5",
        "@types/react": "^18.2.79",
        "@types/react-dom": "^18.2.25",
        "@vitejs/plugin-react": "^4.3.4",
        "autoprefixer": "^10.4.20",
        "openapi-typescript": "^6.7.0",
        "postcss": "^8.4.49",
        "prettier": "^3.3.3",
        "tailwindcss": "^3.4.17",
        "typescript": "~5.9.3",
        "vite": "^7.2.4"
    }
}
